{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "id": "project-root",
    "name": "CSpell Spelling Checker Configuration",
    "description": "CSpell spelling checker settings for the Minecraft Expanded Gamerules Redux project.",
    "import": ["./.cspell/project.json"],
    "loadDefaultConfiguration": true,
    "dictionaries": ["project"],
    "noSuggestDictionaries": [],
    "ignorePaths": [],
    "ignoreWords": [],
    "caseSensitive": false,
    "useGitignore": true,
    "failFast": false,
    "gitignoreRoot": ["./"]
}
